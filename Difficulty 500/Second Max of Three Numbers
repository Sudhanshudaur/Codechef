#include <bits/stdc++.h>
using namespace std;

int main() {
	 int t,x,y,z;
	 cin>>t;
	 for(int i=1; i<=t; i++)
	 {
	     cin>>x>>y>>z;
	     if((x>y && x<z) || (x>z && x<y))  cout<<x<<endl;
	     else if((y>z && y<x) || (y>x && y<z)) cout<<y<<endl;
	     else if((z>x && z<y) || (z>y && z<x)) cout<<z<<endl;
     }
}
