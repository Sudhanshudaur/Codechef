#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,x,n;
	cin>>t;
	for(int i=1; i<=t; i++)
	{
	    cin>>x>>n;
	    if (x*100>n)                 cout<<0<<endl;
	    else if (n%100==0)           cout<<(n-x*100)/100<<endl;
	    else                         cout<<((n-x*100)/100)+1<<endl;
	}
}
