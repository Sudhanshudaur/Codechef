#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,x,y,ans,res;
	cin>>t;
	for(int i=1; i<=t; i++)
	{
	    cin>>x>>y;
	    if (x<y)          cout<<x<<endl;
	    else if (x%y==0)   cout<<x/y<<endl;
	    else {
	    ans = (x/y)*y;
	    res = x/y;
	    while(ans!=x)
	    { ans++;
	      res++; }
	  cout<<res<<endl;   
	   }
    }
}
