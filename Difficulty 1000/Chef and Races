#include <bits/stdc++.h>
using namespace std;

int main() {
	 int t,x,y,a,b, ans = 0;
	 cin>>t;
	 while (t--)
	 {
	     cin>>x>>y>>a>>b;
	     if ((x==a || x==b)&&(y==a || y==b))        cout<<0<<endl;
	     else if ((x==a || x==b)&&(y!=a || y!=b))   cout<<1<<endl;
	     else if ((x!=a || x!=b)&&(y==a || y==b))   cout<<1<<endl;
	     else                                       cout<<2<<endl;
	 }
}
