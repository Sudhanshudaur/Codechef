#include <iostream>
using namespace std;

int main() {
    int t, a, b, x, y;
    cin >> t;
    while (t--) 
    {
        cin >> a >> b >> x >> y;
        if (a * y == b * x)        cout << "Both" << endl;
        else if (a * y > b * x)    cout << "Chefina" << endl;
        else                       cout << "Chef" << endl;
    }
}
