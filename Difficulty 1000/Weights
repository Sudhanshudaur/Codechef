#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,w,x,y,z;
	cin>>t;
	for(int i=1; i<=t; i++)
	{
	    cin>>w>>x>>y>>z;
	    if (x==w||y==w||z==w)                   cout<<"YES"<<endl;
	    else if (w==(x+y)||w==(y+z)||w==(x+z)||w==(x+y+z)) 
	    cout<<"YES"<<endl;
	    else                                    cout<<"NO"<<endl;
	}
}
