#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,n;  cin>>t;
	while (t--)
	{
	    cin>>n;  int cnt=0;
	    for (int i=2; i<n/2; i++)
	    {
	        if (n%i==0)  cnt++;
	        if (cnt>1)   break;
	    }
	       if (cnt!=0 || n==1)  cout<<"no"<<endl;
	       else                 cout<<"yes"<<endl;
	}
}
