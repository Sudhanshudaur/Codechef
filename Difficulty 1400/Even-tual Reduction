#include <bits/stdc++.h>
using namespace std;

int main() {
    int t, n;
    cin >> t;
    while (t--)
    {
        cin >> n;
        string s;
        cin >> s;
        bool b = true;
        map < char, int > chk;
        for (int i = 0; i < n; i++)
        {
            char c = s[i];
            if (chk.find(c) == chk.end()) {
                chk[c] = 1;
            }
            else chk[c]++;
        }
        for (const auto i: chk) {

            if ((i.second % 2) == 1) {
                b = false;
                break;
            }
        }
        if (b) cout << "YES" << endl;
        else cout << "NO" << endl;
    }
}
