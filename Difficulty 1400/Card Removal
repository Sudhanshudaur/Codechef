#include <bits/stdc++.h>
#include <map>
using namespace std;

int main() {
	int t,n;  cin>>t;
	while(t--)
	{
	    cin>>n;
	    int a[n];
	    for(int i=0; i<n; i++)  {  cin>>a[i];  }
	    map<int, int> cnt;
	    for (int num : a) {  cnt[num]++;  }
	    int mxval = 0, key =  0;
	    for(auto pair : cnt)
	    {
	      if (pair.second>mxval)  mxval = pair.second;
	      if (pair.second==mxval) key = pair.first; 
	    }
	    cout<<n-mxval<<endl;
	}
}
